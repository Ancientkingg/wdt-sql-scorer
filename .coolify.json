{
	"name": "SQL Query Reviewer",
	"type": "dockerfile",
	"dockerfile": "Dockerfile",
	"port": 3000,
	"healthcheck": {
		"test": [
			"CMD",
			"wget",
			"--no-verbose",
			"--tries=1",
			"--spider",
			"http://localhost:3000/health"
		],
		"interval": "30s",
		"timeout": "10s",
		"retries": 5,
		"start_period": "60s"
	}
}
